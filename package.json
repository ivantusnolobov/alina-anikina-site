{
  "name": "alina-anikina-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "pretty-quick": "pretty-quick --staged",
    "format": "prettier --write --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|html|css|md)\"",
    "validate": "npm-run-all --parallel check-types pretty-quick lint build"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "gray-matter": "^4.0.3",
    "html-react-parser": "^3.0.1",
    "micromark": "^3.0.10",
    "micromark-extension-gfm": "^2.0.1",
    "next": "12.2.2",
    "octokit": "^2.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.4",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
