{
  "name": "alina-anikina-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "pretty-quick": "pretty-quick --staged",
    "format": "prettier --write --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|html|css|md)\"",
    "validate": "npm-run-all --parallel check-types pretty-quick lint build"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "gray-matter": "^4.0.3",
    "htmr": "^1.0.1",
    "next": "12.1.6",
    "octokit": "^1.7.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "snarkdown": "^2.0.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "17.0.41",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "4.7.3"
  }
}
